;; my key binding
(global-set-key [f11] 'hide-subtree)
(global-set-key [f12] 'show-subtree)
(global-set-key [f2] 'man)
(global-set-key [f4] 'planner-create-task-from-buffer)
(global-set-key [f7] 'flyspell-region)
(global-set-key [f8] 'compile)
(global-set-key [f9] 'remember)
(global-set-key [f10] 'remember-region)

